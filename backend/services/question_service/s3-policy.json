{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ListBucketIfNeeded",
      "Effect": "Allow",
      "Action": ["s3:ListBucket"],
      "Resource": "arn:aws:s3:::peerprep-question-service"
    },
    {
      "Sid": "ObjectRW",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:HeadObject",
        "s3:DeleteObject"
      ],
      "Resource": "arn:aws:s3:::peerprep-question-service/*"
    }
  ]
}
